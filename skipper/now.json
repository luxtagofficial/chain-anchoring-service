{
    "name": "cas-skipper",
    "public": false,
    "version": 2,
    "builds": [
        {
            "src": "now-handler/*.ts",
            "use": "@now/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
    	{ 
            "src": "(.*)",
            "dest": "now-handler/api.ts",
            "methods": [
              "GET",
              "OPTIONS"
            ],
            "headers": {
              "Access-Control-Allow-Origin": "*",
              "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept",
              "Access-Control-Allow-Credentials": "true"
            }
        }
    ]
}
