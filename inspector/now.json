{
  "name": "cas-inspector",
  "public": false,
  "version": 2,
  "builds": [
    {
      "src": "now-handler/*.ts",
      "use": "@now/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/anchors",
      "dest": "now-handler/api.ts",
      "methods": [
        "POST",
        "OPTIONS"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    { "src": "/(.+)/(.+)", "dest": "now-handler/$1.ts" },
    { "src": "/(.+)/", "dest": "now-handler/$1.ts" },
    { "src": "/(.+)", "dest": "now-handler/$1.ts" }
  ]
}